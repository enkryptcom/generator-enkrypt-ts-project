{
  "name": "generator-enkrypt-ts-project",
  "version": "0.1.5",
  "description": "Typescript scaffholding for enkrypt projects",
  "homepage": "https://www.enkrypt.com",
  "author": {
    "name": "kvhnuke",
    "email": "10602065+kvhnuke@users.noreply.github.com"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "yeoman-generator",
    "typescript",
    "prettier",
    "eslint"
  ],
  "devDependencies": {
    "@types/chai": "^4.3.3",
    "@types/mocha": "^9.1.1",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-xo": "^0.41.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.8",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^6.3.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "chai": "^4.3.6",
    "chalk": "^2.4.2",
    "yeoman-generator": "^5.7.0",
    "yosay": "^2.0.2"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "jest"
  },
  "repository": "git@github.com:enkryptcom/generator-ts-project.git",
  "license": "MIT"
}
